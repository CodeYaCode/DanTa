<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.snda.cloud.virtulization.resource.dao.IResourcePoolDao">
	<resultMap type="resourcePool" id="resourcePool">
		<result property="poolId" column="POOL_ID" />
		<result property="poolName" column="POOL_NAME" />
		<result property="schedulePolicy" column="SCHEDULE_POLICY" />
		<result property="deleted" column="DELETED" />
	</resultMap>
	<select id="selectAllResourcePool"  resultMap="resourcePool">
		SELECT
			POOL_ID, POOL_NAME, SCHEDULE_POLICY, DELETED
		FROM
			RESOURCE_POOL
		WHERE
			DELETED != 1
	</select>

</mapper>